<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="lazyLoadingEnabled" value="false" />
    </settings>
    <typeAliases>
        <typeAlias alias="AppEntity" type="com.jzx.bda.somero.mysql.persistent.entity.AppPara" />
        <typeAlias alias="serviceEntity" type="com.jzx.bda.somero.mysql.persistent.entity.ServicePara" />
        <typeAlias alias="SeedEntity" type="com.jzx.bda.somero.mysql.persistent.entity.SeedData" />
        <typeAlias alias="ServiceIdGenEntity" type="com.jzx.bda.somero.mysql.persistent.entity.ServiceIdGen" />
    </typeAliases>
    <mappers>
        <mapper resource="mybatis/ServiceMapper.xml"/>
        <mapper resource="mybatis/AppMapper.xml"/>
        <mapper resource="mybatis/SeedMapper.xml"/>
        <mapper resource="mybatis/ServiceIdGenMapper.xml"/>
    </mappers>
</configuration>